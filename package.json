{
	"name": "hello-world-netlify-pro",
	"version": "0.0.1",
	"description": "Testing Netlify with a pro setup",
	"main": "index.js",
	"repository": "https://github.com/Offirmo/hello-world-netlify-identity.git",
	"author": "Offirmo <offirmo.net@gmail.com>",
	"license": "Unlicense",
	"private": true,
	"scripts": {
		"build": "rm -rf ./functions && netlify-lambda build ./src/functions",
		"start": "netlify-lambda serve ./src/functions"
	},
	"proxy": {
		"/.netlify/functions": {
			"target": "http://localhost:9000",
			"pathRewrite": {
				"^/\\.netlify/functions": ""
			}
		}
	},
	"dependencies": {
		"netlify-identity-widget": "^1.4.14"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.0.0-rc.2",
		"@types/node": "^8.10",
		"babel-loader": "^7.1.5",
		"eslint": "^5.4.0",
		"netlify-lambda": "^0.4.0",
		"npm-run-all": "^4.1.3",
		"prettier": "^1.14.2",
		"tslib": "^1.9.3",
		"tslint": "^5.11.0",
		"typescript": "^3.0.1",
		"typescript-eslint-parser": "^18.0.0",
		"typescript-string-enums": "^0.3.5"
	}
}
